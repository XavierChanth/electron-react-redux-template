{
  "name": "crank.gg",
  "version": "0.1.0",
  "private": true,
  "description": "A helper for League Client.",
  "author": "XavierChanth",
  "contributors": ["XavierChanth", "Michael-4"],
  "dependencies": {
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "asar": "^2.0.3",
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2",
    "electron-packager": "^14.2.0",
    "replace-in-file": "^5.0.2"
  },
  "build": {
    "appId": "com.electron.tutorialApp",
    "productName": "Electron Tutorial App",
    "target": "NSIS",
    "asar": true,
    "directories": {
      "output": "windows-build",
      "buildResources": "assets"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": "true",
      "oneClick": false
    }
  }
}
